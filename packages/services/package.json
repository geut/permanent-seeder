{
  "name": "@geut/permanent-services",
  "version": "0.0.1",
  "description": "Template to create modules following the Geut path",
  "main": "index.js",
  "files": [
    "lib",
    "bin",
    "index.js"
  ],
  "scripts": {
    "test": "jest --passWithNoTests",
    "posttest": "npm run lint",
    "lint": "xd-ns \"**/*.js\"",
    "version": "chan release --allow-yanked ${npm_package_version} && git add .",
    "prepublishOnly": "npm test"
  },
  "dependencies": {
    "@geut/seeder": "^0.0.1",
    "@geut/seeder-database": "^0.0.1",
    "got": "^11.5.0",
    "moleculer": "^0.14.8",
    "moleculer-cron": "^0.0.2",
    "moleculer-repl": "^0.6.4"
  },
  "devDependencies": {
    "@geut/chan": "^2.0.0",
    "@geut/xd": "^1.5.0",
    "standard": "^14.3.1",
    "jest": "^24.8.0"
  },
  "jest": {
    "testMatch": [
      "**/tests/**/*.test.js"
    ]
  },
  "standard": {
    "env": ["jest", "node", "browser"]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/geut/services.git"
  },
  "keywords": [
    "create",
    "geut",
    "module"
  ],
  "author": {
    "name": "GEUT",
    "email": "contact@geutstudio.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/geut/services/issues"
  },
  "homepage": "https://github.com/geut/services#readme"
}
