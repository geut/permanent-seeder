{
  "name": "@geut/seeder",
  "version": "0.0.1",
  "description": "Template to create modules following the Geut path",
  "main": "index.js",
  "files": [
    "lib",
    "bin",
    "index.js"
  ],
  "scripts": {
    "start": "node index.js",
    "test": "jest --passWithNoTests",
    "posttest": "npm run lint",
    "lint": "xd-ns \"**/*.js\"",
    "version": "chan release --allow-yanked ${npm_package_version} && git add .",
    "prepublishOnly": "npm test"
  },
  "dependencies": {
    "@geut/hyperdrive-promise": "^3.0.1",
    "corestore": "^5.6.0",
    "corestore-swarm-networking": "^5.5.1",
    "moleculer": "^0.14.8",
    "moleculer-repl": "^0.6.4",
    "moleculer-web": "^0.9.1",
    "random-access-file": "^2.1.4"
  },
  "devDependencies": {
    "@geut/chan": "^2.2.1",
    "@geut/xd": "^1.6.0",
    "jest": "^24.8.0",
    "standard": "^14.3.4"
  },
  "jest": {
    "testMatch": [
      "**/tests/**/*.test.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/geut/seeder.git"
  },
  "keywords": [
    "create",
    "geut",
    "module"
  ],
  "author": {
    "name": "GEUT",
    "email": "contact@geutstudio.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/geut/seeder/issues"
  },
  "homepage": "https://github.com/geut/seeder#readme",
  "standard": {
    "env": [
      "jest",
      "node"
    ]
  }
}
